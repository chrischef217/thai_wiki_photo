{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "thaiwiki",
  "compatibility_date": "2025-09-11",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "thaiwiki-production",
      "database_id": ""
    }
  ],
  "r2_buckets": [
    {
      "binding": "BUCKET",
      "bucket_name": "thaiwiki-files"
    }
  ]
}